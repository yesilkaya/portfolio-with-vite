{
  "compilerOptions": {
    "outDir": "./build",
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "types": ["node"]
  },
  "include": ["backend/**/*", "env.d.ts", "node_modules/@types"]
}
